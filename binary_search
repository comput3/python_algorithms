def binary_search(x, y):
    first = 0
    last = len(x) - 1
    found = False
    while(first <= last and not found):
        mid = (first + last)//2
        if x[mid]==y:
            found = True
        else:
            if y < x[mid]:
                last = mid - 1
            else:
                first = mid + 1
    return found

binary_search(list(range(1,int(1e6))), 69)
